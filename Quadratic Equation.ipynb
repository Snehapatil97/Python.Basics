   "source": [
    "import cmath\n",
    "\n",
    "a=2\n",
    "b=5\n",
    "c=7\n",
    "\n",
    "#calculate the discriminant\n",
    "d = (b**2) - (4*a*c)\n",
    "\n",
    "#find two  solutions\n",
    "sol1 = (-b-cmath.sqrt(d))/(2*a)\n",
    "sol2 = (-b+cmath.sqrt(d))/(2*a)\n",
    "\n",
    "print('The solutions are {0} and {1}'.format(sol1,sol2))\n",
    "\n",
    "print(\"The solution for quadratic eqquation is\", (sol1,sol2))\n"
   ]
